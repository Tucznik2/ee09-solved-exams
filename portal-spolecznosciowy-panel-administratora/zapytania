1. SELECT id, imie, nazwisko, rok_urodzenia, zdjecie FROM osoby LIMIT 30;
2. SELECT imie, nazwisko, rok_urodzenia, opis, zdjecie, nazwa FROM osoby INNER JOIN hobby ON osoby.Hobby_id=hobby.id WHERE osoby.id = 10;
3. SELECT AVG(rok_urodzenia) AS sredni_rok, MIN(rok_urodzenia) AS min_rok, MAX(rok_urodzenia) AS max_rok FROM osoby;
4. UPDATE osoby SET opis = "muzyka" WHERE Hobby_id = 4;